{
  "name": "weibo-koa2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node bin/www",
    "dev": "cross-env NODE_ENV=dev ./node_modules/.bin/nodemon bin/www",
    "prd": "cross-env NODE_ENV=production pm2 start bin/www",
    "test": "cross-env NODE_ENV=test jest --runInBand --forceExit --colors",
    "watch-serve": "cross-env NODE_ENV=dev ./node_modules/.bin/nodemon --watch './src/**/*' --ext ts,tsx  --exec ts-node  bin/www",
    "tsdev": "tsc --watch & export NODE_ENV=dev && node bin/dev.js -t dist/ -e dist/app.js",
    "rldev": "cross-env NODE_ENV=dev ./node_modules/.bin/nodemon bin/www --watch './src/**/*'"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "ejs": "~2.3.3",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-convert": "^1.2.0",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.0",
    "koa-onerror": "^4.1.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa-views": "^6.2.0",
    "mysql2": "^2.1.0",
    "sequelize": "^5.21.5"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/ejs": "^3.0.1",
    "@types/koa": "^2.11.2",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-json": "^2.0.18",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-router": "^7.4.0",
    "@types/koa-static": "^4.0.1",
    "@types/koa-views": "^2.0.4",
    "@types/node": "^13.7.7",
    "@types/sequelize": "^4.28.8",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "nodemon": "^1.19.1",
    "typescript": "^3.8.3"
  }
}
